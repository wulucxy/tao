<section class="tab-box <%= klass %> current detailContent">
<% if (wishes.length > 0) { %>

	<% for (var i = 0; i < wishes.length; i++) { 
			var list = wishes[i];
		%>

		<span class="badge squre wishBadge">
			<% if (list.assessment == '-2') { %>
				不建议
			<% }else if (list.assessment == '-1') { %>
			   	保守
			<% }else if (list.assessment == 0) { %>
			   	平稳
			<% }else if (list.assessment == 1) { %>
			   	冲刺
			<% } %>
		</span>

		<div class="caseSection">
			<h3>
				
			<a href="/library/major/<%= list.majorId %>" class="textLink" target="_blank">${list.majorName}
			</a>

			<small class="g9">
				<%= list.batch %>
				<% if (list.field){ %>
					(<%= list.field %>) 
				<% } %>
			</small></h3>
			<h4 class="name badgeRow">
				<a class="badgetitle vm textLink"  
				href='/library/college/<%= list.collegeId %>'
				target="_blank" ><%= list.collegeName %></a>
				<% for (var k = 0; k < list.feature.length; k++) { %>
					<span class="badge"><%= list.feature[k].name %></span>
				<% } %>
			</h4>
			<div class="detail">
		<i class="icon icon-city"></i><span class="field"><%= list.city %></span>
		<span class="label">院校分类：</span><span class="field"><%= list.type %></span>
		<span class="label">院校性质：</span><span class="field"><%= list.ownerType %></span>
		<span class="label">院校层次：</span><span class="field"><%= list.level %></span>
		</div>

	<% if (list.scoreList.length > 0) { %>
		<div class="tableWrap">
			<div class="orange tc f24 mt10 mb10">2016年录取情况</div>
				<table class="table table-bordered text-center">
					<thead>
						<tr>
							<td width="240"></td>
							<td width="240">平均分</td>
							<td width="140">省控线分差</td>
							<td width="140">学制</td>
							<td width="140">人数</td>
						</tr>
					</thead>
					<tbody>
						<% for (var j = 0; j < list.scoreList.length; j++) { 
							var score = list.scoreList[j];
						%>
						<tr>
							<td>
							<% if (score.courseType == 1) { %>
							   		文科
							<% }else if (score.courseType == 0) { %>
							   		理科
							<% } %>
							</td>
					<td><%= score.admittedScore %>分</td>
					<td><%= score.diffScore %>分</td>
					<td><%= score.eductionalSystme %>年</td>
					<td><%= score.recruitCount %>人</td>
					</tr>
					<% } %>
					</tbody>
				</table>
		</div>
	<% }else{ %>
			<div class="f16 g3 empty">暂无历史数据</div>
	<% } %>
	</div>
<% } %>		
<% }else{ %>
	<div class="f16 g3">暂无数据</div>
<% } %>
</section>
