<% if (codes.length == 0) { %>
	<li class="no_transList"><i class="noListIcon"></i><em class="vm">暂无记录</em></li>
<% }else{ %>	
<% for (var i = 0; i < codes.length; i++) { %>
<li class="clearfix">
	<div class="well clearfix" type="1">
	<div class="media fl">
		<p><em class="label">授权码：</em><em className="field"><%= codes[i].code %></em></p>
		<p><em class="label">生成日期：</em><em className="field"><%= codes[i].date %></em></p>
	</div>
	<div class="detailInfo fr">
		<div class="row btnRow"><a href="#" class="btn btn-primary btn-medium" targe="_blank">查看</a></div>
	</div>
</div>
</li>
<% }} %>