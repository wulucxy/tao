<% if (news.length == 0 && page == 1) { %>
	<li class="no_transList"><i class="noListIcon"></i><em class="vm">暂无记录</em></li>
<% }else{ %>
<% for (var i = 0; i < news.length; i++) { %>
<li>
   	 <div class="media">
		<span class="fl imgWrap">
			<img src="<%= news[i].newsIconUrl %>">
		</span>
		<div class="media-body">
				<h3 class="clearfix">
					<a class="detailTitle ellipsis" href="/infoV3/<%= news[i].newsId %>/fillPolicDetail" target="_blank">
						<%= news[i].newsTitle %>
					</a>
				</h3>
				<div class="detailCnt clearfix">
					<a href="/infoV3/<%= news[i].newsId %>/fillPolicDetail" class="dib" target="_blank">
						<%= news[i].newsTitle %>
					</a>
					<span class="moment"><%= news[i].date %></span>
				</div>
		</div>
	</div>
</li>
<% }} %>